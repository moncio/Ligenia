# API Configuration
# The base URL for the backend API
VITE_API_BASE_URL=http://localhost:4000

# Supabase Configuration
# The Supabase project URL - get this from your Supabase dashboard
VITE_SUPABASE_URL=https://your-project-id.supabase.co
# The Supabase anonymous key (public) - get this from your Supabase dashboard
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Feature Flags
# Enable or disable specific features
VITE_ENABLE_AI_ASSISTANT=true
VITE_ENABLE_ADVANCED_STATISTICS=true

# Performance
# Configure performance settings
VITE_API_CACHE_TIME=300000 # 5 minutes in milliseconds

# Deployment Environment
# Used to conditionally render features or enable debugging
VITE_ENVIRONMENT=development # Options: development, staging, production 